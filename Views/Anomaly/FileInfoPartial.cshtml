@model Q_verify_2025.Models.FileInfoModel

@if (Model != null)
{
    <div class="mt-3">
        <h5>Filinformation:</h5>
        <ul>
            <li><strong>Filnamn:</strong> @Model.FileName</li>
            <li><strong>Storlek:</strong> @Model.FileSize KB</li>
            <li><strong>Format:</strong> @Model.FileFormat</li>
            <li><strong>Uppladdad:</strong> @Model.UploadTime</li>
        </ul>
    </div>
}
