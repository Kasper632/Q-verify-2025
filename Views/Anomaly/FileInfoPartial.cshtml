@model Q_verify_2025.Models.FileInfoModel

@if (Model != null)
{
    <div class="alert alert-secondary mt-3 d-flex flex-wrap">
        <span class="me-3"><strong>Fil:</strong> @Model.FileName</span>
        <span class="me-3"><strong>Storlek:</strong> @Model.FileSize KB</span>
        <span class="me-3"><strong>Format:</strong> @Model.FileFormat</span>
        <span class="me-3"><strong>Uppladdad:</strong> @Model.UploadTime</span>
    </div>
}
